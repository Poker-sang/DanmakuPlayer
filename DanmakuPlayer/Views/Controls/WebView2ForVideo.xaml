<UserControl
    x:Class="DanmakuPlayer.Views.Controls.WebView2ForVideo"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:DanmakuPlayer.Views.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <WebView2
        CanGoBack="{x:Bind CanGoBack, Mode=TwoWay}"
        CanGoForward="{x:Bind CanGoForward, Mode=TwoWay}"
        PointerReleased="WebView2Tapped" />
</UserControl>
